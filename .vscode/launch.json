            "name": "flutter_application_1 (profile mode)",
            "cwd": "my_helper_app\\flutter_application_1",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "flutter_application_1 (release mode)",
            "cwd": "my_helper_app\\flutter_application_1",
